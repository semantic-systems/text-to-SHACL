@prefix ex: <http://example.org/ns#> .
@prefix ff: <https://foerderfunke.org/default#> .

# Does not meet requirement "As a result of the insurance event, user is unfit for work"

ff:User ff:hasInsurance ff:StatutoryAccidentInsurance ;
    ff:hasEvent ex:SomeEvent .

ex:SomeEvent a ff:WorkAccident ;
    ff:reducesEarningCapacity [
        ff:unfitForWork false
    ] .