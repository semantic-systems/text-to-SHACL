@prefix ex: <http://example.org/ns#> .
@prefix ff: <https://foerderfunke.org/default#> .

# Does not meet requirement "User is employed in Germany OR employed abroad and subject to social security in Germany"

ff:User ff:hasEmploymentStatus ff:Employed ;
    ff:countryOfEmployment ex:Tuvalu ;
    ff:socialSecurityCountry ex:Tuvalu ;
    ff:employedBy ex:TheBusiness .

ex:TheBusiness ff:hasEvent ex:Event123 .

ex:Event123 a ff:InsolvencyProceeding;
    ff:wageCutOffsetMonths -3;
    ff:isOpen true.