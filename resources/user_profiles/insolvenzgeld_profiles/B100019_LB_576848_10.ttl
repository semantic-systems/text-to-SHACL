@prefix ex: <http://example.org/ns#> .
@prefix ff: <https://foerderfunke.org/default#> .

# Does not meet requirement "An insolvency event occurred at the user's employer" (InsolvencyApplicationShape)

ff:User ff:hasEmploymentStatus ff:Employed ;
    ff:countryOfEmployment ff:Germany ;
    ff:employedBy ex:TheBusiness .

ex:TheBusiness ff:hasEvent ex:Event123 .

ex:Event123 a ff:RejectionInsolvencyApplication;
    ff:reason "Formal errors"@en ;
    ff:wageCutOffsetMonths -1.