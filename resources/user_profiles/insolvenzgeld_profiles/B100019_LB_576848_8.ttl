@prefix ex: <http://example.org/ns#> .
@prefix ff: <https://foerderfunke.org/default#> .

# Does not meet requirement "User wage was cut within 3 months before the insolvency event"

ff:User ff:hasEmploymentStatus ff:Employed ;
    ff:countryOfEmployment ff:Germany ;
    ff:employedBy ex:TheBusiness .

ex:TheBusiness ff:hasEvent ex:Event123 .

ex:Event123 a ff:InsolvencyProceeding;
    ff:monthsWithWageCutBeforeEvent 1; # Offset "+1", i.e. wage cut occured 1 month AFTER event
    ff:isOpen true.