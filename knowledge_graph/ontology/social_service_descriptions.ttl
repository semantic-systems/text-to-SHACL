@prefix : <http://visualdataweb.org/social-service-descriptions/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://foerderfunke.org/default#> .

<http://visualdataweb.org/social-service-descriptions/> rdf:type owl:Ontology ;
                                                         dc:description "An ontology that conceptualizes social service descriptions from the perspectives of personal information relevant to determine whether an individual may be elgibile for the service."@en ;
                                                         dc:title "Social Service Descriptions"@en .

#################################################################
#    Annotation properties
#################################################################

rdfs:comment rdf:type owl:AnnotationProperty .
rdfs:label rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

<hasChild> rdf:type owl:ObjectProperty ;
           rdfs:domain <Person> ;
           rdfs:range <Child> ;
           rdfs:label "has child"@en .

<hasMaritalStatus> rdf:type owl:ObjectProperty ;
                   rdfs:domain <Person> ;
                   rdfs:range <MaritalStatus> ;
                   rdfs:label "has marital status"@en .

<sameHouseholdAs> rdf:type owl:ObjectProperty, owl:SymmetricProperty ;
                  rdfs:domain <Person> ;
                  rdfs:range <Person> ;
                  rdfs:label "lives in same household as"@en .

<expectedPerformance> rdf:type owl:ObjectProperty ;
                      rdfs:domain <Person> ;
                      rdfs:range <Performance> ;
                      rdfs:comment "Indicates the expected job performance of Person compared to an average employee."@en ;
                      rdfs:label "expected performance"@en .


#################################################################
#    Data properties
#################################################################

<bruttoIncome> rdf:type owl:DatatypeProperty ;
               rdfs:domain <Person> ;
               rdfs:range xsd:float ;
               rdfs:comment "Person's brutto income in Euros."@en ;
               rdfs:label "brutto income"@en .

<childBenefit> rdf:type owl:DatatypeProperty ;
               rdfs:domain <Child> ;
               rdfs:range xsd:float ;
               rdfs:comment "The amount of child benefit that Child receives in Euros. 0 if it receives no child benefit."@en ;
               rdfs:label "child benefit"@en .

<hasAge> rdf:type owl:DatatypeProperty ;
         rdfs:domain <Person> ;
         rdfs:range xsd:integer ;
         rdfs:comment "Person's age expressed as an integer."@en ;
         rdfs:label "age"@en .

<singleParent> rdf:type owl:DatatypeProperty ;
               rdfs:domain <Person> ;
               rdfs:range xsd:boolean ;
               rdfs:comment "True if Person is a single parent."@en ;
               rdfs:label "single parent"@en .

<checkedPriorityClaims> rdf:type owl:DatatypeProperty ;
                        rdfs:domain <Person> ;
                        rdfs:range xsd:boolean ;
                        rdfs:comment "True if Person has checked claims that have priority over the current claim."@en ;
                        rdfs:label "checked priority claims"@en .

<integrationRequiresSubsidy> rdf:type owl:DatatypeProperty ;
                             rdfs:domain <Person> ;
                             rdfs:range xsd:boolean ;
                             rdfs:comment "True if financial support is necessary to integrate Person long-term."@en ;
                             rdfs:label "integration requires subsidy"@en .

#################################################################
#    Classes
#################################################################

<Person> rdf:type owl:Class ;
         rdfs:label "Person"@en .

<User> rdf:type owl:Class ;
       rdfs:subClassOf <Person> ;
       rdfs:label "User"@en .

<Child> rdf:type owl:Class ;
        rdfs:subClassOf <Person> ;
        rdfs:label "Child"@en .

<Employer> rdf:type owl:Class ;
           rdfs:subClassOf <Person> ;
           rdfs:label "Employer"@en .

<MaritalStatus> rdf:type owl:Class ;
                rdfs:label "Marital status"@en .

<Married> rdf:type owl:Class ;
          rdfs:subClassOf <MaritalStatus> ;
          rdfs:label "Married"@en ;
          owl:disjointWith <RegisteredPartnership>,<Single> .

<RegisteredPartnership> rdf:type owl:Class ;
                        rdfs:subClassOf <MaritalStatus> ;
                        rdfs:label "Registered partnership"@en ;
                        owl:disjointWith <Single> .

<Single> rdf:type owl:Class ;
         rdfs:subClassOf <MaritalStatus> ;
         rdfs:label "Single"@en .

<Performance> rdf:type owl:Class ;
              rdfs:comment "Indicates job performance compared to an average employee."@en ;
              rdfs:label "Performance"@en .

<BelowAveragePerformance> rdf:type owl:Class ;
                          rdfs:subClassOf <Performance> ;
                          rdfs:label "Below average performance"@en ;
                          owl:disjointWith <AveragePerformance>,<AboveAveragePerformance> .

<AveragePerformance> rdf:type owl:Class ;
                     rdfs:subClassOf <Performance> ;
                     rdfs:label "Average performance"@en ;
                     owl:disjointWith <AboveAveragePerformance> .

<AboveAveragePerformance> rdf:type owl:Class ;
                          rdfs:subClassOf <Performance> ;
                          rdfs:label "Above average performance"@en .